{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./dist",
      "strict": true,
      "declaration": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strictPropertyInitialization": false,
    },
    "include": ["*.ts"],
    "exclude": ["node_modules"]
  }